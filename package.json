{
    "name": "toon-scrape",
    "version": "1.2.0",
    "description": "Scrape web cartoon to new web.",
    "author": "MrNiranam",
    "license": "MIT",
    "scripts": {
        "postinstall": "run-p postinstall:**",
        "postinstall:frontend": "cd frontend && npm install",
        "postinstall:functions": "cd functions && npm install",
        "build": "run-p build:**",
        "build:frontend": "cd frontend && npm run build",
        "build:functions": "netlify-lambda build functions/src",
        "start": "run-p start:**",
        "start:frontend": "cd frontend && npm run start",
        "start:functions": "netlify-lambda serve functions/src"
    },
    "dependencies": {},
    "devDependencies": {
        "netlify-lambda": "^1.4.5",
        "npm-run-all": "^4.1.5"
    }
}
